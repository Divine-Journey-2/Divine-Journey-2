// ensure we dont put the same tooltip on the same item twice.
// has to be clunky like this because we dont have access to the event
static check as IItemStack[][string] = {};

function add(item as IItemStack, type as string) as void {
    if (!(check has type)) check[type] = [];
    // this doesnt work properly for items with nbt tags, so skip those and presume they are correct
    if (!item.hasTag && check[type] has item) return;
    check[type] = check[type] + item;
    item.addTooltip("8o#" ~ type ~ "r");
}

// add the default tags
for key, value in tags {
    for item in value {
        add(item, key);
    }
}

// some tags are things we would consider subsets - add them directly here to avoid duplication of work
for item in tags.helmet { add(item, "armor"); }
for item in tags.chestplate { add(item, "armor"); }
for item in tags.leggings { add(item, "armor"); }
for item in tags.boots { add(item, "armor"); }
for item in tags.power { add(item, "transportation"); }
for item in tags.fluid { add(item, "transportation"); }
for item in tags.item { add(item, "transportation"); }

